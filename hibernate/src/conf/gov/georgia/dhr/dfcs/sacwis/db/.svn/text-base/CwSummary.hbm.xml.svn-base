<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-cascade="none" default-access="property" default-lazy="true" auto-import="true">
    <class name="gov.georgia.dhr.dfcs.sacwis.db.CwSummary" table="CW_SUMMARY" mutable="true" polymorphism="implicit" dynamic-update="false" dynamic-insert="false" select-before-update="false" optimistic-lock="version">
        <id name="idStage" type="integer" unsaved-value="undefined">
            <column name="ID_STAGE" precision="16" scale="0"/>
            <generator class="foreign">
<param name="property">stage</param>
</generator>
        </id>
        <timestamp name="dtLastUpdate" column="DT_LAST_UPDATE" generated="always" unsaved-value="undefined" source="vm"/>
<one-to-one name="stage" class="gov.georgia.dhr.dfcs.sacwis.db.Stage" constrained="true"/>
        
        <many-to-one name="capsCase" class="gov.georgia.dhr.dfcs.sacwis.db.CapsCase" fetch="select" unique="false" update="true" insert="true" optimistic-lock="true" not-found="exception" embed-xml="true">
            <column name="ID_CASE" precision="16" scale="0" not-null="true"/>
        </many-to-one>
        <property name="dtLastOverallCwBatchRun" type="date" unique="false" optimistic-lock="true" lazy="false" generated="never">
            <column name="DT_LAST_OVERALL_CW_BATCH_RUN" length="7"/>
        </property>
        <property name="dtLastAfcarsErrorsRun" type="date" unique="false" optimistic-lock="true" lazy="false" generated="never">
            <column name="DT_LAST_AFCARS_ERRORS_RUN" length="7"/>
        </property>
        <property name="dtLastNcandsErrorsRun" type="date" unique="false" optimistic-lock="true" lazy="false" generated="never">
            <column name="DT_LAST_NCANDS_ERRORS_RUN" length="7"/>
        </property>
        <property name="nbrOpenErrors" type="java.lang.Integer" unique="false" optimistic-lock="true" lazy="false" generated="never">
            <column name="NBR_OPEN_ERRORS" precision="3" scale="0"/>
        </property>
        <property name="nbrOpenWarnings" type="java.lang.Integer" unique="false" optimistic-lock="true" lazy="false" generated="never">
            <column name="NBR_OPEN_WARNINGS" precision="3" scale="0"/>
        </property>
    </class>
</hibernate-mapping>
