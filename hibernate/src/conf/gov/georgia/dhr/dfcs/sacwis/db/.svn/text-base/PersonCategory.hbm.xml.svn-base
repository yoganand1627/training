<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-cascade="none" default-access="property" default-lazy="true" auto-import="true">
  <class name="gov.georgia.dhr.dfcs.sacwis.db.PersonCategory" table="PERSON_CATEGORY" mutable="true"
         polymorphism="implicit" dynamic-update="false" dynamic-insert="false" select-before-update="false"
         optimistic-lock="version">
    <id name="idPersonCategory" type="integer" unsaved-value="0">
      <column name="ID_PERSON_CATEGORY" precision="16" scale="0"/>
      <generator class="sequence">
        <param name="sequence">SEQ_PERSON_CATEGORY</param>
      </generator>
    </id>
    <timestamp name="dtLastUpdate" column="DT_LAST_UPDATE" generated="always" unsaved-value="undefined" source="vm"/>
    <many-to-one name="person" class="gov.georgia.dhr.dfcs.sacwis.db.Person" fetch="select" unique="false" update="true"
                 insert="true" optimistic-lock="true" not-found="exception" embed-xml="true">
      <column name="ID_PERSON" precision="16" scale="0"/>
    </many-to-one>
    <property name="cdPersonCategory" type="string" unique="false" optimistic-lock="true" lazy="false"
              generated="never">
      <column name="CD_PERSON_CATEGORY" length="3"/>
    </property>
  </class>
</hibernate-mapping>
