<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-cascade="none" default-access="property" default-lazy="true" auto-import="true">
  <class name="gov.georgia.dhr.dfcs.sacwis.db.DiversionConclusionNarr" table="DIVERSION_CONCLUSION_NARR" mutable="true"
         polymorphism="implicit" dynamic-update="false" dynamic-insert="false" select-before-update="false"
         optimistic-lock="version">
    <id name="idEvent" type="integer" unsaved-value="undefined">
      <column name="ID_EVENT" precision="16" scale="0"/>
      <generator class="foreign">
        <param name="property">event</param>
      </generator>
    </id>
    <timestamp name="dtLastUpdate" column="DT_LAST_UPDATE" generated="always" unsaved-value="undefined" source="vm"/>
    <one-to-one name="event" class="gov.georgia.dhr.dfcs.sacwis.db.Event" constrained="true"/>
    <many-to-one name="documentTemplate" class="gov.georgia.dhr.dfcs.sacwis.db.DocumentTemplate" fetch="select"
                 unique="false" update="true" insert="true" optimistic-lock="true" not-found="exception"
                 embed-xml="true">
      <column name="ID_DOCUMENT_TEMPLATE" precision="16" scale="0"/>
    </many-to-one>
    <many-to-one name="capsCase" class="gov.georgia.dhr.dfcs.sacwis.db.CapsCase" fetch="select" unique="false"
                 update="true" insert="true" optimistic-lock="true" not-found="exception" embed-xml="true">
      <column name="ID_CASE" precision="16" scale="0"/>
    </many-to-one>
    <property name="narrative" type="binary" unique="false" optimistic-lock="true" lazy="false" generated="never">
      <column name="NARRATIVE"/>
    </property>
  </class>
</hibernate-mapping>
