<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-cascade="none" default-access="property" default-lazy="true" auto-import="true">
  <class name="gov.georgia.dhr.dfcs.sacwis.db.EmpTempAssign" table="EMP_TEMP_ASSIGN" mutable="true"
         polymorphism="implicit" dynamic-update="false" dynamic-insert="false" select-before-update="false"
         optimistic-lock="version">
    <id name="idEmpTempAssign" type="integer" unsaved-value="0">
      <column name="ID_EMP_TEMP_ASSIGN" precision="16" scale="0"/>
      <generator class="sequence">
        <param name="sequence">SEQ_EMP_TEMP_ASSIGN</param>
      </generator>
    </id>
    <timestamp name="dtLastUpdate" column="DT_LAST_UPDATE" generated="always" unsaved-value="undefined" source="vm"/>
    <many-to-one name="personByIdPersonDesignee" class="gov.georgia.dhr.dfcs.sacwis.db.Person" fetch="select"
                 unique="false" update="true" insert="true" optimistic-lock="true" not-found="exception"
                 embed-xml="true">
      <column name="ID_PERSON_DESIGNEE" precision="16" scale="0" not-null="true"/>
    </many-to-one>
    <many-to-one name="personByIdPersonEmp" class="gov.georgia.dhr.dfcs.sacwis.db.Person" fetch="select" unique="false"
                 update="true" insert="true" optimistic-lock="true" not-found="exception" embed-xml="true">
      <column name="ID_PERSON_EMP" precision="16" scale="0" not-null="true"/>
    </many-to-one>
    <property name="dtAssignExpiration" type="date" unique="false" optimistic-lock="true" lazy="false"
              generated="never">
      <column name="DT_ASSIGN_EXPIRATION" length="7"/>
    </property>
  </class>
</hibernate-mapping>
