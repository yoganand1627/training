                 unique="false" update="true" insert="true" optimistic-lock="true" not-found="exception"
                 embed-xml="true">
      <column name="ID_PERSON" precision="16" scale="0" not-null="true"/>
    </many-to-one>
    <property name="cdEthnicity" type="string" unique="false" optimistic-lock="true" lazy="false" generated="never">
      <column name="CD_ETHNICITY" length="2" not-null="true"/>
    </property>
  </class>
</hibernate-mapping>
