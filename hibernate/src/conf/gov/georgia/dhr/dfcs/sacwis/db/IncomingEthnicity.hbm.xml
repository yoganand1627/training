<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-cascade="none" default-access="property" default-lazy="true" auto-import="true">
  <class name="gov.georgia.dhr.dfcs.sacwis.db.IncomingEthnicity" table="INCOMING_ETHNICITY" mutable="true"
         polymorphism="implicit" dynamic-update="false" dynamic-insert="false" select-before-update="false"
         optimistic-lock="version">
    <id name="idIncomingEthnicity" type="integer" unsaved-value="0">
      <column name="ID_INCOMING_ETHNICITY" precision="16" scale="0"/>
      <generator class="sequence">
        <param name="sequence">SEQ_INCOMING_ETHNICITY</param>
      </generator>
    </id>
    <timestamp name="dtLastUpdate" column="DT_LAST_UPDATE" generated="always" unsaved-value="undefined" source="vm"/>
