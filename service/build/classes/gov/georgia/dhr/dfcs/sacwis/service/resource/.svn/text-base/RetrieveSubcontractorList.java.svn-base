package gov.georgia.dhr.dfcs.sacwis.service.resource;

import gov.georgia.dhr.dfcs.sacwis.structs.input.CCON15SI;
import gov.georgia.dhr.dfcs.sacwis.structs.output.CCON15SO;

public interface RetrieveSubcontractorList {
  /**
   * Retrieves all resources that have been designated as subcontractors for the prime resource. It will also retrieve
   * all services for which the prime resource may provide.
   *
   * @param ccon15si The input object containing the method parameters.
   * @return CCON15SO The output object containing the retrieved row/column values.
   */
  public CCON15SO findSubcontractorResources(CCON15SI ccon15si);
}
