package gov.georgia.dhr.dfcs.sacwis.service.security;

import gov.georgia.dhr.dfcs.sacwis.structs.input.CARC12SI;
import gov.georgia.dhr.dfcs.sacwis.structs.output.CARC12SO;

public interface RetrieveSecurityClass {

  /**
   * Retrieves a list of SecurityClass objects and orders them by securityclass name.
   *
   * @param carc12si
   * @return All security classes in a {@link CARC12SO} object.
   */
  CARC12SO retrieveSecurityClass(CARC12SI carc12si);
}
