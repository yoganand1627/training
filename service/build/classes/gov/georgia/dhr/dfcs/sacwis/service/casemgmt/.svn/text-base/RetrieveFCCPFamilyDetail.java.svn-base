package gov.georgia.dhr.dfcs.sacwis.service.casemgmt;

import gov.georgia.dhr.dfcs.sacwis.structs.input.FCCPFamilyDetailSI;
import gov.georgia.dhr.dfcs.sacwis.structs.output.FCCPFamilyDetailSO;

public interface RetrieveFCCPFamilyDetail {
  /**
   * Retrieves information for the family case plan from FCCP_FAMILY, PLAN_STEP, PLAN_PARTICIPANT, 
   * @param fccpFamilyDetailSI
   * @return FCCPFamilyDetailSO object containing relevant data for the selected family case plan 
   */
  FCCPFamilyDetailSO retrieveFCCPFamilyDetail(FCCPFamilyDetailSI fccpFamilyDetailSI);
  
  /**
   * Retrieves the data  
   * @param fccpFamilyDetailSI
   * @return FCCPFamilyDetailSO object containing relevant data for the selected family case plan 
   */
  byte[] retrieveFCCPForm(FCCPFamilyDetailSI fccpFamilyDetailSI);
}
