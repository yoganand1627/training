package gov.georgia.dhr.dfcs.sacwis.service.admin;

import gov.georgia.dhr.dfcs.sacwis.structs.input.CCMN02UI;
import gov.georgia.dhr.dfcs.sacwis.structs.output.CCMN02UO;

public interface CloseStageCase {

  /**
   * This shared library function provides the necessary updates required to close a stage. If a case and a situation
   * are associated with the stage, and there are no other open stages associated with the case, the situation and the
   * case are also closed.
   *
   * @param ccmn02ui
   * @return {@link CCMN02UI}
   */
  public CCMN02UO closeStageCase(CCMN02UI ccmn02ui);
}
