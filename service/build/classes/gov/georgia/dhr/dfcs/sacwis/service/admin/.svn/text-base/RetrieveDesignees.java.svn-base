package gov.georgia.dhr.dfcs.sacwis.service.admin;

import gov.georgia.dhr.dfcs.sacwis.structs.input.CARC16SI;
import gov.georgia.dhr.dfcs.sacwis.structs.output.CARC16SO;

public interface RetrieveDesignees {

  /**
   * This service retrieves all designees for a given employee. It performs a full row retrieval of the EMP_TEMP_ASSIGN
   * table for a given ID PERSON.
   *
   * @param carc16si {@link CARC16SI} object
   * @return a {@link CARC16SO} object
   */
  public CARC16SO retrieveDesigneeAssignments(CARC16SI carc16si);
}
