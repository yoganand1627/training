  protected Bookmark createBookmark(String name, String data) {
    Bookmark bookmark = new Bookmark();
    bookmark.setBookmarkName(name);
    bookmark.setBookmarkData(data == null ? "" : StringEscapeUtils.escapeJava(data));
    return bookmark;
  }

  protected Bookmark createBookmark(String name, Number data) {
    Bookmark bookmark = new Bookmark();
    bookmark.setBookmarkName(name);
    bookmark.setBookmarkData(data == null ? "" : String.valueOf(data));
    return bookmark;
  }

  protected Bookmark createBookmark(String name, Object data) {
    Bookmark bookmark = new Bookmark();
    bookmark.setBookmarkName(name);
    if ((String.valueOf(data).equalsIgnoreCase(NULL_DATE)) || (String.valueOf(data)).equals(ZERO)) {
      bookmark.setBookmarkData("");
    } else {
      bookmark.setBookmarkData(data == null ? "" : String.valueOf(data));
    }
    return bookmark;
  }

  protected Bookmark createBookmarkFromMap(Map map, String field, String key, String codesTable) {
    Object value = map.get(key);
    Object data = codesTable == null ? value : Lookup.simpleDecodeSafe(codesTable, (String) value);
    return createBookmark(field, data);
  }

  protected Bookmark createBookmarkWithCodesTable(String field, Object value, String codesTable) {
    Object data = codesTable == null ? value : Lookup.simpleDecodeSafe(codesTable, (String) value);
    return createBookmark(field, data);
  }
  

