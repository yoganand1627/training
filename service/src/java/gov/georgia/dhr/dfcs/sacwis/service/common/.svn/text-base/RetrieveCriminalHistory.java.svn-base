package gov.georgia.dhr.dfcs.sacwis.service.common;

import gov.georgia.dhr.dfcs.sacwis.structs.input.CCFC31SI;
import gov.georgia.dhr.dfcs.sacwis.structs.output.CCFC31SO;

public interface RetrieveCriminalHistory {

  /**
   * This service will retrieve all rows from the Criminal History Table for a given ID_REC_CHECK. The maximum page size
   * retrieved is 10 rows.
   *
   * @param ccfc31si
   * @return {@link CCFC31SO}
   */

  public CCFC31SO retrieveCriminalHistory(CCFC31SI ccfc31si);

}