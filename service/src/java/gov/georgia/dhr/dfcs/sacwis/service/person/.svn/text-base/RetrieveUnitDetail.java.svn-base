import gov.georgia.dhr.dfcs.sacwis.structs.output.CCMN23SO;

public interface RetrieveUnitDetail {

  /**
   * This service is designed to retrieve the list of employees assigned to a particular unit.  It receives ID UNIT, ID
   * PERSON for the user and the user's designees, and SYS CD WIN MODE.  It returns data from the EMP JOB HISTORY,
   * PERSON, and UNIT EMP LINK tables.
   *
   * @param ccmn23si
   */
  public CCMN23SO findUnitEmployees(CCMN23SI ccmn23si);

}
