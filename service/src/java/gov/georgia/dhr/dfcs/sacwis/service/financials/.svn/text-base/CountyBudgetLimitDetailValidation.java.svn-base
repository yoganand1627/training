package gov.georgia.dhr.dfcs.sacwis.service.financials;

public interface CountyBudgetLimitDetailValidation {

  /**
   * Conversation calls this method before saving a new CountyBudgetLimitDetail object
   * Inserts a new one only if there isnt a CountyBudgetLimitDetail for the given arguments.
   * @param program
   * @param fiscalYear
   * @return
   */
  int isCountyBudgetLimitDetailValid(String county, String program, int fiscalYear);
}
