<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
  <bean id="dataSource" class="org.springframework.jdbc.datasource.DriverManagerDataSource">
    <property name="driverClassName" value="${jdbc.driverClassName}"/>
    <property name="url" value="${jdbc.url}"/>
    <property name="username" value="${jdbc.username}"/>
    <property name="password" value="${jdbc.password}"/>
    <!--<property name="suppressClose" value="false"/>-->
  </bean>

  <!-- This provides values for ${} variables above. -->
  <bean id="placeholderConfig" class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
    <property name="location" value="jdbc.properties"/>
  </bean>

  <!-- Transaction management-->
  <bean id="transactionManager" class="org.springframework.orm.hibernate3.HibernateTransactionManager">
    <property name="sessionFactory" ref="sessionFactory"/>
  </bean>

  <!-- Session factory for testing. -->
  <bean id="sessionFactory" class="org.springframework.orm.hibernate3.LocalSessionFactoryBean">
    <property name="dataSource" ref="dataSource"/>
    <property name="configLocation" value="classpath:hibernate.cfg.xml"/>
    <property name="hibernateProperties">
      <props>
        <prop key="hibernate.dialect">org.hibernate.dialect.Oracle9Dialect</prop>
      </props>
    </property>
  </bean>

  <!-- This creates proxies for beans at runtime in order to make them use transactions declaratively. -->
  <bean id="autoServiceProxyCreator" class="gov.georgia.dhr.dfcs.sacwis.service.spring.ServiceAutoProxyCreator">
    <property name="interceptorNames">
      <list>
        <idref local="txInterceptor"/>
        <idref bean="sessionFlushAdvisor"/>
      </list>
    </property>
  </bean>

  <!-- An advisor that automatically transforms common Hibernate exceptions into Service Exceptions. -->
  <bean id="hibernateAdvisor" class="org.springframework.aop.support.DefaultPointcutAdvisor">
    <property name="advice" ref="hibernateThrowsAdvice"/>
  </bean>

  <!-- Note: This bean definition does not contain definitions for transactionManager; it must be provided another file. -->
  <bean id="txInterceptor" class="org.springframework.transaction.interceptor.TransactionInterceptor">
    <property name="transactionManager" ref="transactionManager"/>
    <property name="transactionAttributeSource" ref="transactionAttributeSource"/>
  </bean>

  <!-- This bean defines a map between INTERFACE names (not implementation names) and methods and their transaction
       requirements. Possible values are:
         o PROPAGATION_REQUIRED: Executes inside an existing transaciton, if available; otherwise, starts a new one.
         o PROPAGATION_SUPPORTS: Execute inside an existing transaction, if available; otherwise, executes with no transaction.
         o PROPAGATION_MANDATORY: Executes inside an existing transaction, if available; otherwise, throws an exception.
         o PROPAGATION_REQUIRES_NEW: Always creats a new transaction.
         o PROPAGATION_NOT_SUPPORTED: Executes with no transaction; suspeds existing transaction if one is open.
         o PROPAGATION_NEVER: Executes with no transaction; throws an exception if one exists.
         o PROPAGATION_NESTED: Run as a nested transaction if one exists; if none exists, execute as PROPAGATION_REQUIRED.
       In most cases, PROPAGATION_REQUIRED is appropriate. If you use something else, please include a comment as to why.
       If the transaction is known to be readOnly, ",readOnly" should be included after the transaction attribute attribute.-->
  <bean id="transactionAttributeSource"
        class="org.springframework.transaction.interceptor.MethodMapTransactionAttributeSource">
    <property name="methodMap">
