package gov.georgia.dhr.dfcs.sacwis.web.financials;

import java.io.Serializable;

public class InvoicePaginationDB
        implements Serializable {
  protected int ulIdInvoice = 0;
  protected int pageNumber = 0;
  protected int rowIndex = 0;

  public InvoicePaginationDB(int ulIdInvoice,
                             int pageNumber) {
    this.ulIdInvoice = ulIdInvoice;
    this.pageNumber = pageNumber;
  }

  public int getUlIdInvoice() {
    return ulIdInvoice;
  }

  public int getPageNumber() {
    return pageNumber;
  }

  public int getRowIndex() {
    return rowIndex;
  }

  public void setRowIndex(int rowIndex) {
    this.rowIndex = rowIndex;
  }

  public String toString() {
    return
            "InvoicePaginationDB: \n" +
            " ulIdInvoice: " + ulIdInvoice + "\n" +
            " pageNumber: " + pageNumber + "\n" +
            " rowIndex: " + rowIndex + "\n" +
            "End InvoicePaginationDB \n";
  }
}

  
