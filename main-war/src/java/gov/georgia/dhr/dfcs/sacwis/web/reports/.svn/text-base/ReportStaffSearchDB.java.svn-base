package gov.georgia.dhr.dfcs.sacwis.web.reports;


import java.io.Serializable;

import java.util.List;

/** Used by ReportParameters to maintain state when StaffSearch is executed */
public class ReportStaffSearchDB implements Serializable {

	protected String staffName;

	protected String nameOfReport;

	protected String hdnNmRptSqrName;

	protected String hdnNmRptSqrVer;

	protected List parameterList;
    
	protected int staffId;
	
	protected String szTxtNmRptType;
	
	public String getStaffName() {
		return staffName;
	}

	public void setStaffName(String staffName) {
		this.staffName = staffName;
	}

	public String getNameOfReport() {
		return nameOfReport;
	}

	public void setNameOfReport(String nameOfReport) {
		this.nameOfReport = nameOfReport;
	}

	public List getParameterList() {
		return parameterList;
	}

	public void setParameterList(List parameterList) {
		this.parameterList = parameterList;
	}

	public String getHdnNmRptSqrName() {
		return hdnNmRptSqrName;
	}

	public void setHdnNmRptSqrName(String hdnNmRptSqrName) {
		this.hdnNmRptSqrName = hdnNmRptSqrName;
	}

	public String getHdnNmRptSqrVer() {
		return hdnNmRptSqrVer;
	}

	public void setHdnNmRptSqrVer(String hdnNmRptSqrVer) {
		this.hdnNmRptSqrVer = hdnNmRptSqrVer;
	}
	
	public void setStaffId(int staffId){
		this.staffId = staffId;
	}
	
	public int getStaffId(){
		return staffId;
	}

  public String getSzTxtNmRptType() {
    return szTxtNmRptType;
  }

  public void setSzTxtNmRptType(String szTxtNmRptType) {
    this.szTxtNmRptType = szTxtNmRptType;
  }
}
