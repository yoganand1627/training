package gov.georgia.dhr.dfcs.sacwis.core.history;

// -- C-IV Architecture classes --

import gov.georgia.dhr.dfcs.sacwis.core.exception.ArchitectureException;
import gov.georgia.dhr.dfcs.sacwis.core.exception.BasePrsException;

/**
 * A SessioinHistoryException is thrown when there is an error processing Action or History Objects
 *
 * @author Phillip T. Bernard
 */
public class SessionHistoryException extends ArchitectureException {
  public SessionHistoryException(Exception e) {
    super(null, e, BasePrsException.INFORMATIONAL_PRIORITY);
  }

  public SessionHistoryException(String message, Exception e) {
    super(message, e, BasePrsException.INFORMATIONAL_PRIORITY);
  }
}












