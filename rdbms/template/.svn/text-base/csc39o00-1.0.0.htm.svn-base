									<td class="style8">[[PART_INFO_NAME]]</td>
									<td class="style8">[[PART_INFO_DATE_OF_NOTIF]]</td>
									<td class="style8">[[PART_INFO_TYPE_OF_NOTIF]]</td>
									<td class="style8">[[PART_INFO_DATE_OF_PARTCP]]</td>
									<td class="style8">[[PART_INFO_RELTNSP_TO_CASE]]</td>
