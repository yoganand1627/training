-- change STGAP00003999
update CAPS.MESSAGE set TXT_MESSAGE = 'You have indicated this is a ''Notification of Change''.  Please enter comments detailing the change in circumstance.' where NBR_MESSAGE = 60178 and TXT_MESSAGE_NAME = 'MSG_FCE_NOC_COMMENTS_REQ';
update CAPS.MESSAGE set TXT_MESSAGE = 'The question ''Is the child in the process of being adopted?'' must be answered.' where NBR_MESSAGE = 60177 and TXT_MESSAGE_NAME = 'MSG_FCE_ADOPTION_QUES_REQ';
update CAPS.MESSAGE set TXT_MESSAGE = 'The question ''Is the child approaching emancipation?'' must be answered.' where NBR_MESSAGE = 60176 and TXT_MESSAGE_NAME = 'MSG_FCE_EMANCIPATION_QUES_REQ';
update CAPS.MESSAGE set TXT_MESSAGE = 'The question ''Is the child expected to be in out-of-home care for a short period of time?'' must be answered.' where NBR_MESSAGE = 60175 and TXT_MESSAGE_NAME = 'MSG_FCE_OUT_HOME_QUES_REQ';
update CAPS.MESSAGE set TXT_MESSAGE = 'The question ''Does the child''s cost of care exceed $853.00?'' must be answered.' where NBR_MESSAGE = 60174 and TXT_MESSAGE_NAME = 'MSG_FCE_COST_EXCEED_QUES_REQ';
update CAPS.MESSAGE set TXT_MESSAGE = 'You have indicated the child has a Personal Savings account outside of DFCS.  Please add a resource of type ''Savings Account.''' where NBR_MESSAGE = 25621 and TXT_MESSAGE_NAME = 'MSG_CHILDS_SAVINGS_ACCOUNT_REQ';
update CAPS.MESSAGE set TXT_MESSAGE = 'The question, ''Was child support court ordered?'' must be answered.' where NBR_MESSAGE = 25410 and TXT_MESSAGE_NAME = 'MSG_CHILD_SUP_QUES_REQ';

