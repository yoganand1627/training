package gov.georgia.dhr.dfcs.sacwis.structs.input;

import java.io.Serializable;

public class FosterHomeConversionChildBean implements Serializable {

  private String childName;
  
  private String childGender;
  
  private int childAge;
  
  private int idChild;
  
  private int idFosterHomeConvPerLink;
  
  private int idEvent;
  
  public FosterHomeConversionChildBean() {
    super();
  }

  public String getChildName() {
    return childName;
  }

  public void setChildName(String childName) {
    this.childName = childName;
  }

  public String getChildGender() {
    return childGender;
  }

  public void setChildGender(String childGender) {
    this.childGender = childGender;
  }

  public int getChildAge() {
    return childAge;
  }

  public void setChildAge(int childAge) {
    this.childAge = childAge;
  }

  public int getIdChild() {
    return idChild;
  }

  public void setIdChild(int idChild) {
    this.idChild = idChild;
  }

  public int getIdFosterHomeConvPerLink() {
    return idFosterHomeConvPerLink;
  }

  public void setIdFosterHomeConvPerLink(int idFosterHomeConvPerLink) {
    this.idFosterHomeConvPerLink = idFosterHomeConvPerLink;
  }

  public int getIdEvent() {
    return idEvent;
  }

  public void setIdEvent(int idEvent) {
    this.idEvent = idEvent;
  }

}
