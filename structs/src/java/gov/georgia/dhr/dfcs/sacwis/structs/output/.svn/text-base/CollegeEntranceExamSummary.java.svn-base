package gov.georgia.dhr.dfcs.sacwis.structs.output;

import java.io.Serializable;
import java.util.Date;

/**
 * This file is to hold College Entrance Exam's summary data to display in the College Entrance Exams list section on
 * Youth Detail page. The data is retrieved from College_Exam table using id person from Youth Detail page
 * 
 * @author hong.van.t.vo
 * 
 */
@SuppressWarnings("serial")
public class CollegeEntranceExamSummary implements Serializable {
  private Date dtExamTaken = null;

  private String cdExamType = "";

  private int idExam = 0;

  public int getIdExam() {
    return idExam;
  }

  public void setIdExam(int idExam) {
    this.idExam = idExam;
  }

  public String getCdExamType() {
    return cdExamType;
  }

  public void setCdExamType(String cdExamType) {
    this.cdExamType = cdExamType;
  }

  public Date getDtExamTaken() {
    return dtExamTaken;
  }

  public void setDtExamTaken(Date dtExamTaken) {
    this.dtExamTaken = dtExamTaken;
  }

}
