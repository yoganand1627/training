package gov.georgia.dhr.dfcs.sacwis.structs.output;

import java.io.Serializable;
import java.util.Date;

public class CountyBudgetLimitRetrieveSO implements Serializable {

  private String county;
  private String program;
  private double budgetedAmount;
  private double amountSpent;
  private double amountObligated;
  private double remainingBalance;
  private int idCountyBudgetLimit;
  private int fiscalYear;
  private Date dtLastUpdate = null;

  public Date getDtLastUpdate() {
    return dtLastUpdate;
  }

  public void setDtLastUpdate(Date dtLastUpdate) {
    this.dtLastUpdate = dtLastUpdate;
  }

  public int getFiscalYear() {
    return fiscalYear;
  }

  public void setFiscalYear(int fy) {
    fiscalYear = fy;
  }

  public String getCounty() {
    return county;
  }

  public void setCounty(String c) {
    this.county = c;
  }

  public String getProgram() {
    return program;
  }

  public void setProgram(String p) {
    this.program = p;
  }

  public double getBudgetedAmount() {
    return budgetedAmount;
  }

  public void setBudgetedAmount(double amt) {
    budgetedAmount = amt;
  }

  public double getAmountSpent() {
    return amountSpent;
  }

  public void setAmountSpent(double amt) {
    amountSpent = amt;
  }

  public double getAmountObligated() {
    return amountObligated;
  }

  public void setAmountObligated(double amt) {
    amountObligated = amt;
  }

  public double getRemainingBalance() {
    return remainingBalance;
  }

  public void setRemainingBalance(double amt) {
    remainingBalance = amt;
  }

  public int getIdCountyBudgetLimit() {
    return idCountyBudgetLimit;
  }

  public void setIdCountyBudgetLimit(int idCountyBudgetLimit) {
    this.idCountyBudgetLimit = idCountyBudgetLimit;
  }
}
