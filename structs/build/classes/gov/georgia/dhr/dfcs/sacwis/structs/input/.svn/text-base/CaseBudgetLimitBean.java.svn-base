  private String cdSvcCode;

  private Double amtBudgt;

  private Double amtSpent;

  private Double amtRemain;

  private Double amtWaiver;

  private Double amtBalance;

  private Double amtPendAuth;

  public Double getAmtPendAuth() {
    return amtPendAuth;
  }

  public void setAmtPendAuth(Double amtPendAuth) {
    this.amtPendAuth = amtPendAuth;
  }

  public Double getAmtBalance() {
    return amtBalance;
