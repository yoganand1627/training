package gov.georgia.dhr.dfcs.sacwis.dao;

import java.util.Date;

public interface ComplexPersonPhoneDAO {
  /**
   * Does a full row add and updates of the PERSON_PHONE table.  UPDATEs do not modify DT_PERSON_PHONE_START
   *
   * @param personPhoneEnd
   * @param idPerson
   * @param cdPhoneType
   * @param cdNbrPhone
   * @param cdNbrPhoneExtension
   * @param indPersonPhonePrimary
   * @param indPersonPhoneInvalid
   * @param cdTxtPhoneComments
   * @param idPhone
   * @param tsLastUpdate
   */
  int insertPersonPhone(Date personPhoneEnd, int idPerson, String cdPhoneType, String cdNbrPhone,
                        String cdNbrPhoneExtension, String indPersonPhonePrimary, String indPersonPhoneInvalid,
                        String cdTxtPhoneComments, int idPhone, Date tsLastUpdate);

  /**
   * Does a full row add and updates of the PERSON_PHONE table.  UPDATEs do not modify DT_PERSON_PHONE_START
   *
   * @param personPhoneEnd
   * @param idPerson
   * @param cdPhoneType
   * @param cdNbrPhone
   * @param cdNbrPhoneExtension
   * @param indPersonPhonePrimary
   * @param indPersonPhoneInvalid
   * @param cdTxtPhoneComments
   * @param idPhone
   * @param tsLastUpdate
   */
  int updatePersonPhone(Date personPhoneEnd, int idPerson, String cdPhoneType, String cdNbrPhone,
                        String cdNbrPhoneExtension, String indPersonPhonePrimary, String indPersonPhoneInvalid,
                        String cdTxtPhoneComments, int idPhone, Date tsLastUpdate);
}
