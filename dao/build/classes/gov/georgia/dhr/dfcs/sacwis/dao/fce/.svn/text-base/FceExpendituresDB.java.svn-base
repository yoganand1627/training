  public void copyInto(FceExpendituresDB fceExpendituresDB) {

    if (hasIdFceExpenditures) {
      fceExpendituresDB.setIdFceExpenditures(idFceExpenditures);
    }
    if (hasIdFceEligibility) {
      fceExpendituresDB.setIdFceEligibility(idFceEligibility);
    }
    if (hasIdPerson) {
      fceExpendituresDB.setIdPerson(idPerson);
    }
    if (hasIdFcePerson) {
      fceExpendituresDB.setIdFcePerson(idFcePerson);
    }
    if (hasNmServiceProvider) {
      fceExpendituresDB.setNmServiceProvider(nmServiceProvider);
    }
    if (hasAmtPdMonthly) {
      fceExpendituresDB.setAmtPdMonthly(amtPdMonthly);
    }
    if (hasDtLastUpdate) {
      fceExpendituresDB.setDtLastUpdate(dtLastUpdate);
    }
    if (hasIdStage) {
      fceExpendituresDB.setIdStage(idStage);
    }
    if (hasIdLastUpdatePerson) {
      fceExpendituresDB.setIdLastUpdatePerson(idLastUpdatePerson);
    }
    if (hasIdPersonCareReceive) {
      fceExpendituresDB.setIdPersonCareReceive(idPersonCareReceive);
    }
  }

  public static void populateWithMap(FceExpendituresDB fceExpendituresDB,
                                     Map map) {
    if (map.containsKey("AMT_PD_MONTHLY")) {
      Number value = (Number) map.get("AMT_PD_MONTHLY");
      fceExpendituresDB.setAmtPdMonthly(value);
    }
    if (map.containsKey("DT_LAST_UPDATE")) {
      Date value = (Date) map.get("DT_LAST_UPDATE");
      fceExpendituresDB.setDtLastUpdate(value);
    }
    if (map.containsKey("ID_FCE_EXPENDITURES")) {
      Number value = (Number) map.get("ID_FCE_EXPENDITURES");
      fceExpendituresDB.setIdFceExpenditures(value);
    }
    if (map.containsKey("ID_FCE_ELIGIBILITY")) {
      Number value = (Number) map.get("ID_FCE_ELIGIBILITY");
      fceExpendituresDB.setIdFceEligibility(value);
    }
    if (map.containsKey("ID_FCE_PERSON")) {
      Number value = (Number) map.get("ID_FCE_PERSON");
      fceExpendituresDB.setIdFcePerson(value);
    }
    if (map.containsKey("ID_LAST_UPDATE_PERSON")) {
      Number value = (Number) map.get("ID_LAST_UPDATE_PERSON");
      fceExpendituresDB.setIdLastUpdatePerson(value);
    }
    if (map.containsKey("ID_PERSON")) {
      Number value = (Number) map.get("ID_PERSON");
      fceExpendituresDB.setIdPerson(value);
    }
    if (map.containsKey("ID_STAGE")) {
      Number value = (Number) map.get("ID_STAGE");
      fceExpendituresDB.setIdStage(value);
    }
    if (map.containsKey("NM_SERVICE_PROVIDER")) {
      String value = (String) map.get("NM_SERVICE_PROVIDER");
      fceExpendituresDB.setNmServiceProvider(value);
    }
    if (map.containsKey("ID_PERSON_CARE_RECEIVE")) {
      Number value = (Number) map.get("ID_PERSON_CARE_RECEIVE");
      fceExpendituresDB.setIdPersonCareReceive(value);
    }
  }

