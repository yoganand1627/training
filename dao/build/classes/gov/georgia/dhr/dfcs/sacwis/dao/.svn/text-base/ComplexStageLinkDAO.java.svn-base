package gov.georgia.dhr.dfcs.sacwis.dao;

public interface ComplexStageLinkDAO {
  
  /**
   * Gets the most recently closed previous ID STAGE for a given ID STAGE.
   * If one doesn't exist, it will return a zero(0)
   * 
   * @param idStage
   * @return
   */
  Integer findPreviousIdStageByIdStage(int idStage);
  
  /**
   * Gets the most recently closed previous ID STAGE for a given ID STAGE.
   * If one doesn't exist, it will return a zero(0)
   * 
   * @param idStage
   * @return
   */
  Integer findPreviousIdStageByIdStageByCdStage(int idStage);
}
