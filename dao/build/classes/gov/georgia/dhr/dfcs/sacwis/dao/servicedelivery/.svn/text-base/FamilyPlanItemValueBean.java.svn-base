package gov.georgia.dhr.dfcs.sacwis.dao.servicedelivery;

import java.util.Collection;
import java.util.Date;

import gov.georgia.dhr.dfcs.sacwis.core.base.BaseValueBean;

/** @todo add javadoc comments */

/**
 * Holds all data pertaining to a Family Plan Item.
 *
 * @author Jason Rios, February 6th, 2003
 */
public class FamilyPlanItemValueBean extends BaseValueBean {
  // instance variables
  private int caseId;
  private int stageId;
  private int familyPlanEventId;
  private int familyPlanItemId;
  private Date familyPlanItemDateLastUpdate;
  private String areaOfConcernCode;
  private String areaOfConcernText;
  private String initialLevelOfConcernScale;
  private String currentLevelOfConcernScale;
  private String goals;
  private Date dateInitiallyAddressed;
  private Date dateGoalsCompleted;
  private boolean identifiedInRiskAssessment;
  private Collection<FamilyPlanTaskValueBean> tasks;
  private Collection evalItems;

  public String getAreaOfConcernCode() {
    return areaOfConcernCode;
  }

  public String getAreaOfConcernText() {
    return areaOfConcernText;
  }

  public int getCaseId() {
    return caseId;
  }

  public String getCurrentLevelOfConcernScale() {
    return currentLevelOfConcernScale;
  }

  public Date getDateInitiallyAddressed() {
    return dateInitiallyAddressed;
  }
  
  public Date getDateGoalsCompleted() {
    return dateGoalsCompleted;
  }

  public Collection getEvalItems() {
    return evalItems;
  }

  public String getGoals() {
    return goals;
  }

  public boolean isIdentifiedInRiskAssessment() {
    return identifiedInRiskAssessment;
  }

  public String getInitialLevelOfConcernScale() {
    return initialLevelOfConcernScale;
  }

  public Date getFamilyPlanItemDateLastUpdate() {
    return familyPlanItemDateLastUpdate;
  }

  public int getFamilyPlanItemId() {
    return familyPlanItemId;
  }

  public int getFamilyPlanEventId() {
    return familyPlanEventId;
  }

  public int getStageId() {
    return stageId;
  }

  public Collection<FamilyPlanTaskValueBean> getTasks() {
    return tasks;
  }

  public void setAreaOfConcernCode(String areaOfConcernCode) {
    this.areaOfConcernCode = areaOfConcernCode;
  }

  public void setAreaOfConcernText(String areaOfConcernText) {
    this.areaOfConcernText = areaOfConcernText;
  }

  public void setCaseId(int caseId) {
    this.caseId = caseId;
  }

  public void setCurrentLevelOfConcernScale(String currentLevelOfConcernScale) {
    this.currentLevelOfConcernScale = currentLevelOfConcernScale;
  }

  public void setDateGoalsCompleted(Date dateGoalsCompleted) {
    this.dateGoalsCompleted = dateGoalsCompleted;
  }
  
  public void setDateInitiallyAddressed(Date dateInitiallyAddressed) {
    this.dateInitiallyAddressed = dateInitiallyAddressed;
  }

  public void setEvalItems(Collection evalItems) {
    this.evalItems = evalItems;
  }

  public void setGoals(String goals) {
    this.goals = goals;
  }

  public void setIdentifiedInRiskAssessment(boolean identifiedInRiskAssessment) {
    this.identifiedInRiskAssessment = identifiedInRiskAssessment;
  }

  public void setInitialLevelOfConcernScale(String initialLevelOfConcernScale) {
    this.initialLevelOfConcernScale = initialLevelOfConcernScale;
  }

  public void setFamilyPlanItemDateLastUpdate(Date familyPlanItemDateLastUpdate) {
    this.familyPlanItemDateLastUpdate = familyPlanItemDateLastUpdate;
  }

  public void setFamilyPlanItemId(int familyPlanItemId) {
    this.familyPlanItemId = familyPlanItemId;
  }

  public void setFamilyPlanEventId(int familyPlanEventId) {
    this.familyPlanEventId = familyPlanEventId;
  }

  public void setStageId(int stageId) {
    this.stageId = stageId;
  }

  public void setTasks(Collection<FamilyPlanTaskValueBean> tasks) {
    this.tasks = tasks;
  }
