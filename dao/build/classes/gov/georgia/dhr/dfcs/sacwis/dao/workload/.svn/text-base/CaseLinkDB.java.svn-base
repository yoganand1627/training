package gov.georgia.dhr.dfcs.sacwis.dao.workload;

import gov.georgia.dhr.dfcs.sacwis.core.base.BaseValueBean;

/**
 * This CaseLinkDB Performs the database activities to get/set the values also store it in the CASE LINK table.
 *
 * @author Vijaya Anand
 * @version 1.0
 *          <p/>
 *          Change History: Date      User         Description -------  -----------  ----------------------------------------------
 *          04/08/05  ANANDV       SIR 23522 - CaseLinkDB for PrincipalCaseHistory Page
 */

public class CaseLinkDB
        extends BaseValueBean {

  private int caseLinkSeqID;
  private int mainCaseID;
  private int linkCaseID;
  private int userID;
  private String indCaseLink;
  private int personUpdate;
  private java.sql.Timestamp dateLinked;
  private java.sql.Timestamp dtLastUpdate;

  public CaseLinkDB() {
    // Used to Insert a new record into CASE LINK Table.
    caseLinkSeqID = 0;
  }

  public int getCaseLinkSeqID() {
    return caseLinkSeqID;
  }

  public void setCaseLinkSeqID(int caseLinkSeqID) {
    this.caseLinkSeqID = caseLinkSeqID;
  }

  /** @return Date */
  public int getMainCaseID() {
    return mainCaseID;
  }

  /** @param mainCaseID  */
  public void setMainCaseID(int mainCaseID) {
    this.mainCaseID = mainCaseID;
  }

  /** @return Date */
  public int getLinkCaseID() {
    return linkCaseID;
  }

  /** @param linkCaseID  */
  public void setLinkCaseID(int linkCaseID) {
    this.linkCaseID = linkCaseID;
  }

  /** @return Date */
  public int getUserID() {
    return userID;
  }

  /** @param userID  */

  public void setUserID(int userID) {
    this.userID = userID;
  }

  /** @return Date */
  public java.sql.Timestamp getDateLinked() {
    return dateLinked;
  }

  /** @param dateLinked  */
  public void setDateLinked(java.sql.Timestamp dateLinked) {
    this.dateLinked = dateLinked;
  }

  /** @return String */
  public String getIndCaseLink() {
    return indCaseLink;
  }

  /** @param indCaseLink  */
  public void setIndCaseLink(String indCaseLink) {
    this.indCaseLink = indCaseLink;
  }

  /** @return java.sql.Timestamp */
  public java.sql.Timestamp getDtLastUpdate() {
    return dtLastUpdate;
  }

  /** @param dtLastUpdate  */
  public void setDtLastUpdate(java.sql.Timestamp dtLastUpdate) {
    this.dtLastUpdate = dtLastUpdate;
  }

  /** @return int */
  public int getPersonUpdate() {
    return personUpdate;
  }

  /** @param personUpdate  */
  public void setPersonUpdate(int personUpdate) {
    this.personUpdate = personUpdate;
  }

}