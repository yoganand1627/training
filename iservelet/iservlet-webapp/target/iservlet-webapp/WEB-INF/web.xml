<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
	<servlet><!-- 5 -->
		<servlet-name>homePageServlet</servlet-name>
		<servlet-class>com.naveltech.iservlet.web.servlet.HomePageServlet</servlet-class>
	</servlet>

	<servlet><!-- 5 -->
		<servlet-name>registrationPageServlet</servlet-name>
		<servlet-class>com.naveltech.iservlet.web.servlet.RegistrationServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>ajaxServlet</servlet-name>
		<servlet-class>com.naveltech.iservlet.web.servlet.AjaxServlet</servlet-class>
	</servlet>
	
	<servlet-mapping><!-- 6 -->
		<servlet-name>homePageServlet</servlet-name>
		<url-pattern>/homePage</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping><!-- 6 -->
		<servlet-name>registrationPageServlet</servlet-name>
		<url-pattern>/registrationPage</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping><!-- 6 -->
		<servlet-name>ajaxServlet</servlet-name>
		<url-pattern>/ajaxRequest</url-pattern>
	</servlet-mapping>
	
	
</web-app>
