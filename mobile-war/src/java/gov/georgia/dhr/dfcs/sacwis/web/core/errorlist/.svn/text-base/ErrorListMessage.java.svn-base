package gov.georgia.dhr.dfcs.sacwis.web.core.errorlist;

import java.io.Serializable;

/**
 * <p/>
 * Title: ErrorListMessage </p>
 * <p/>
 * <p/>
 * Description: This class is a container for Message #, Message, Message Name, and Message URL </p>
 *
 * @author Stephan Brauchli
 * @version 1.0
 */
public class ErrorListMessage implements Serializable {

  /** Creates a new ErrorListMessage object. */
  public ErrorListMessage() {
  }

  /**
   * Description Setter method
   *
   * @param msgNumber param
   */
  public void setMessageNum(int msgNum) {
    this.msgNumber = msgNum;
  }

  /**
   * Description Setter method
   *
   * @param msgName param
   */
  public void setMsgName(String msgName) {
    this.msgName = msgName;
  }

  /**
   * Description Setter method
   *
   * @param taskCD param
   */
  public void setTaskCD(String taskCD) {
    this.taskCD = taskCD;
  }

  /**
   * Description Setter method
   *
   * @param errorMessage param
   */
  public void setErrorMessage(String errorMessage) {
    this.errorMessage = errorMessage;
  }

  /**
   * Description Setter method
   *
   * @param msgName param
   */
  public void setMsgURL(String msgURL) {
    this.msgURL = msgURL;
  }

  /**
   * Description Getter method
   *
   * @param returns msgURL
   */
  public String getMsgURL() {
    msgURL = this.msgURL;
    return msgURL;
  }

  /**
   * Description Getter method
   *
   * @param returns msgName
   */
  public String getMsgName() {
    msgName = this.msgName;
    return msgName;
  }

  /**
   * Description Getter method
   *
   * @param returns msgName
   */
  public String getTaskCD() {
    taskCD = this.taskCD;
    return taskCD;
  }

  /**
   * Description Getter method
   *
   * @param returns errorMessage
   */
  public String getErrorMessage() {
    errorMessage = this.errorMessage;
    return errorMessage;
  }

  /**
   * Description Getter method
   *
   * @param returns errorNumber
   */
  public int getmsgNumber() {
    msgNumber = this.msgNumber;
    return msgNumber;
  }

  /* The following variables are accessed in this class */
  protected int msgNumber = 0;
  protected String errorMessage = "";
  protected String msgName = "";
  protected String msgURL = "";
  protected String taskCD = "";

  /********************************************************************************
   **  Declare any static constants.
   ********************************************************************************/
  // static constants

}