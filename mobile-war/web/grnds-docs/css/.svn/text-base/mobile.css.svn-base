@import url("impact.css");

#stageMenu li {
    list-style-type: none;
    font: 9pt normal Arial, Helvetica, Verdana, sans-serif;
    padding: 3px;
}

#stageMenu { clear: both; display: block; float: left; padding: 3px;}

#pageNav li {
    list-style-type: none;
    font: 9pt normal Arial, Helvetica, Verdana, sans-serif;
    padding: 5px 5px 0 3px;
    /*float: left;*/
    margin: 0 2px;
    display: inline;
}

#pageNav ul {
    border-bottom: 10px solid #b5d17c;
    /*text-align: center;*/
    margin-left: 0;
    padding-left: 0;
}

#pageNav {
/*    padding: 3px; */
    margin: 1em 0 0.5em 0;
    /*background: #e2e1c3;*/
    width: 100%;
}

#persSrchDtlTbl {
    clear: both;
    display: block;
    float: left;
}

.lvl1NavTab {
    background: #81994d;
    color: #fff;
}

.lvl1NavTabSelect {
    background: #e2e1c3;
    color: #425813;
}

.lvl2NavSelect {
    background: #b5d17c;
}

.lvl2Nav {
    background: #e2e1c3;
}

.lvl2Nav a, .lvl2NavSelect a {
    color: #00f;
    text-decoration: none;
}

.tab {
    border-left: 1px solid grey;
    border-right: 1px solid grey;
    border-top: 1px solid grey;
    color: #00f;
    text-decoration: none;
}

.navTab {
    background: #81994d;
    color: #fff;
}

.navTabSelect {
    background: #e2e1c3;
    color: #425813;
}

