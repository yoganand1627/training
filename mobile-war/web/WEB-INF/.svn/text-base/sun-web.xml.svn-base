<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-web-app PUBLIC '-//Sun Microsystems, Inc.//DTD Application Server 8.1 Servlet 2.4//EN'
        'http://www.sun.com/software/appserver/dtds/sun-web-app_2_4-1.dtd'>

<sun-web-app>

  <!-- Session is stored in memory.  If app is clustered, sessions are replicated -->
  <session-config>
    <session-manager persistence-type="memory"/>
  </session-config>

  <!-- Note: These options should match those passed directly to JspC during the JSP precompile. -->
  <jsp-config>
    <property name="classdebuginfo" value="true"/>
    <property name="development" value="@DEVELOPMENT@"/>
    <property name="enablePooling" value="false"/>
    <property name="fork" value="true"/>
    <property name="keepgenerated" value="true"/>
    <property name="javaEncoding" value="Windows-1252"/>
    <property name="mappedfile" value="@MAPPED_FILE@"/>
    <property name="trimSpaces" value="@TRIM_SPACES@"/>
    <property name="use-precompiled" value="@USE_PRECOMPILED@"/>
    <property name="xpoweredBy" value="false"/>
  </jsp-config>
</sun-web-app>

